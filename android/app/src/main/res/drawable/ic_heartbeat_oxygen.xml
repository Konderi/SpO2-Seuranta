<?xml version="1.0" encoding="utf-8"?>
<!-- Heartbeat with O2 Icon - Heart rate + oxygen monitoring -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">
    
    <!-- ECG/Pulse wave line -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="@android:color/white"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:pathData="M2,12 
                         L5,12 
                         L7,8 
                         L9,16 
                         L11,12 
                         L13,12" />
    
    <!-- Circle with O2 -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="@android:color/white"
        android:strokeWidth="1.5"
        android:pathData="M18,12 A5,5 0 1,1 18,12.01 Z" />
    
    <!-- Letter O inside circle -->
    <path
        android:fillColor="@android:color/white"
        android:pathData="M16.5,10 
                         A1.2,1.2 0 1,1 16.5,14 
                         A1.2,1.2 0 1,1 16.5,10 Z
                         M16.5,11 
                         A0.5,0.5 0 1,0 16.5,13 
                         A0.5,0.5 0 1,0 16.5,11 Z" />
    
    <!-- Number 2 inside circle -->
    <path
        android:fillColor="@android:color/white"
        android:pathData="M19,10 
                         L20.5,10 
                         A0.8,0.8 0 0,1 20.5,11.6 
                         L19,13 
                         L20.5,13"
        android:strokeColor="@android:color/white"
        android:strokeWidth="0.6"
        android:strokeLineCap="round" />
</vector>
